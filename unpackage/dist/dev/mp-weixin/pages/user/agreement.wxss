.user-index {
  min-height: 100%;
  padding-bottom: 50px;
  background-color: #f7f7f7;
}
.user-index .head {
  position: relative;
  width: 100%;
  height: 411rpx;
  background: url("https://golemo.oss-cn-shenzhen.aliyuncs.com//5f3235360e8c4.png") #ffffff no-repeat center;
  background-size: cover;
}
.user-index .head .top {
  padding: 120rpx 30rpx 0 34rpx;
}
.user-index .head .top .iconshezhi1 {
  font-size: 48rpx;
  color: #ffffff;
}
.user-index .head .top .bar {
  width: 48rpx;
  position: relative;
}
.user-index .head .top .bar .iconxiaoxi {
  font-size: 48rpx;
  color: #ffffff;
}
.user-index .head .top .bar .circle {
  position: absolute;
  right: 0;
  top: 0;
  width: 14rpx;
  height: 14rpx;
  background: #ff4531;
  border: 3rpx solid #ececec;
  border-radius: 50%;
  display: block;
}
.user-index .head .bottom {
  position: relative;
  top: 52rpx;
}
.user-index .head .bottom text {
  font-size: 44rpx;
  color: #ffffff;
  text-align: center;
  display: block;
  margin: 0 auto 38rpx;
}
.user-index .head .bottom image {
  width: 149rpx;
  height: 149rpx;
  background: #ffffff;
  border: 5rpx solid #ffffff;
  box-shadow: 0px 9rpx 32rpx 0px rgba(171, 171, 171, 0.35);
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}
