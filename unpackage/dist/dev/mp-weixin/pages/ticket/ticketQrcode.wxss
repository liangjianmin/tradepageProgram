view {
  box-sizing: border-box;
}
image {
  vertical-align: middle;
}
.column {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.boxsiz {
  box-sizing: border-box;
}
/* 两侧对齐 */
.bothSide {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
/* 平均分布 */
.avarage {
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
/* 水平居中 */
.rowCenter {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
/* 垂直居中 */
.verCenter {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.boxFlex {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.boxFlexs {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
  flex: 2;
}
page {
  background-color: #FADE6F;
}
.ticket_qrcode {
  width: 100%;
  box-sizing: border-box;
  padding: 32rpx;
  font-weight: 500;
}
.ticket_qrcode .tq-box {
  width: 100%;
  height: 756rpx;
  background-color: #FFFFFF;
}
.ticket_qrcode .tq-box .tq-title {
  width: 100%;
  height: 113rpx;
  line-height: 113rpx;
  font-size: 30rpx;
  color: #333333;
  padding-left: 30rpx;
}
.ticket_qrcode .tq-box .tq-body {
  width: 100%;
  height: calc(100% - 113rpx);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  font-size: 36rpx;
}
.ticket_qrcode .tq-box .tq-body .tqb-title {
  color: #333333;
}
.ticket_qrcode .tq-box .tq-body image {
  width: 320rpx;
  height: 320rpx;
}
.ticket_qrcode .tq-box .tq-body .save {
  color: #FF4531;
}
.ticket_qrcode .share {
  width: 100%;
  height: 70rpx;
  background-color: #FF4531;
  line-height: 70rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 30rpx;
  border-radius: 4px;
  margin-top: 40rpx;
}

