view {
  box-sizing: border-box;
}
image {
  vertical-align: middle;
}
.column {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.boxsiz {
  box-sizing: border-box;
}
/* 两侧对齐 */
.bothSide {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
/* 平均分布 */
.avarage {
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
/* 水平居中 */
.rowCenter {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
/* 垂直居中 */
.verCenter {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.boxFlex {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.boxFlexs {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
  flex: 2;
}
page {
  height: 100%;
}
.user-ticket {
  padding-top: 49rpx;
  min-height: 100%;
  background: #FADE6F;
}
.user-ticket .content {
  margin: 0 32rpx;
  height: 960rpx;
  background: url("https://golemo.oss-cn-shenzhen.aliyuncs.com//5f325fa2ed63d.png") no-repeat center;
  background-size: cover;
}
.user-ticket .content .head {
  margin: 0 49rpx;
  padding-top: 44rpx;
  padding-bottom: 37rpx;
}
.user-ticket .content .head .text .t1 {
  width: 298rpx;
  height: 100rpx;
  font-size: 36rpx;
  color: #333333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-weight: bold;
}
.user-ticket .content .head .text .t2 {
  margin-top: 80rpx;
  display: block;
  font-size: 24rpx;
  color: #999999;
}
.user-ticket .content .head .pic {
  width: 139rpx;
  height: 188rpx;
  border-radius: 10rpx;
}
.user-ticket .content .head .pic image {
  width: 139rpx;
  height: 188rpx;
  border-radius: 10rpx;
}
.user-ticket .content .qr-code {
  margin-top: 103rpx;
}
.user-ticket .content .qr-code image {
  width: 320rpx;
  height: 320rpx;
  margin: 0 auto 20rpx;
  display: block;
}
.user-ticket .content .qr-code text {
  text-align: center;
  font-size: 24rpx;
  color: #999999;
  display: block;
}
.user-ticket .btn {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.user-ticket .btn .btn-1 {
  margin: 0 !important;
  padding: 0 !important;
  width: 50% !important;
  height: 98rpx !important;
  line-height: 98rpx !important;
  background: #FFFFFF !important;
  font-size: 30rpx !important;
  color: #FF4531 !important;
  text-align: center;
  display: block;
}
.user-ticket .btn .btn-2 {
  margin: 0 !important;
  padding: 0 !important;
  width: 50% !important;
  height: 98rpx !important;
  line-height: 98rpx !important;
  background: #FF4531 !important;
  border-radius: 10rpx 0px 0px 10rpx !important;
  font-size: 30rpx !important;
  color: #FFFFFF !important;
  text-align: center;
  display: block;
}
.user-ticket .btn .btn-3 {
  margin: 0 !important;
  padding: 0 !important;
  width: 100% !important;
  height: 98rpx !important;
  line-height: 98rpx !important;
  background: #FF4531 !important;
  border-radius: 0 !important;
  font-size: 30rpx !important;
  color: #FFFFFF !important;
  text-align: center;
  display: block;
}
.user-ticket .btn .btn-4 {
  margin: 0 !important;
  padding: 0 !important;
  width: 100% !important;
  height: 98rpx !important;
  line-height: 98rpx !important;
  background: #FFFFFF !important;
