  text-align: center;
  font-size: 24rpx;
  color: #333333;
}
.shade-box {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}
.shade-box .success {
  position: fixed;
  top: 50%;
  left: 0;
  right: 0;
  width: 631rpx;
  height: 379rpx;
  background: #f8f8f8;
  border-radius: 10rpx;
  margin: 0 auto;
  text-align: center;
  z-index: 11;
}
.shade-box .success .wrap {
  position: absolute;
  top: -65rpx;
  width: 555rpx;
  height: 292rpx;
  background: #ffffff;
  box-shadow: 0px 0px 8rpx 1px rgba(0, 0, 0, 0.35);
  border-radius: 10rpx;
}
.shade-box .success .wrap .ico-1 {
  position: absolute;
  top: -13rpx;
  left: 32rpx;
  width: 21rpx;
  height: 41rpx;
  background: url("https://golemo.oss-cn-shenzhen.aliyuncs.com//5f438cb5e1749.png") no-repeat center;
  background-size: cover;
  display: block;
}
.shade-box .success .wrap .ico-2 {
  position: absolute;
  top: -13rpx;
  right: 32rpx;
  width: 21rpx;
  height: 41rpx;
  background: url("https://golemo.oss-cn-shenzhen.aliyuncs.com//5f438cb5e1749.png") no-repeat center;
  background-size: cover;
  display: block;
}
.shade-box .success .wrap .t1 {
  font-size: 45rpx;
  color: #161616;
  text-align: center;
  display: block;
  margin: 0 45rpx;
  padding: 18rpx 0 29rpx 0;
  border-bottom: 1px dashed #d2d2d2;
  font-weight: bold;
}
.shade-box .success .wrap .t2 {
  margin-top: 40rpx;
  margin-bottom: 7px;
  font-size: 27rpx;
  color: #161616;
  display: block;
  padding: 0 30rpx;
  line-height: 38rpx;
  text-align: center;
}
.shade-box .success .wrap .t3 {
  font-size: 30rpx;
  color: #161616;
}
.shade-box .success .btn {
  width: 369rpx;
  height: 67rpx;
  line-height: 67rpx;
  text-align: center;
  background: #30c27c;
  border-radius: 34rpx;
  font-size: 34rpx;
  color: #ffffff;
  margin: 266rpx auto 0;
}
.shade-box .success .close {
  position: absolute;
  bottom: -68rpx;
  width: 56rpx;
  height: 56rpx;
  background: url("https://golemo.oss-cn-shenzhen.aliyuncs.com//5f4390bbe3b07.png") no-repeat center;
  background-size: cover;
  display: block;
}

